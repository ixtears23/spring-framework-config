<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="com.ibdata.myapp" level="DEBUG" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    
    <Logger name="com.ibdata.project" level="DEBUG" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    
    <Logger name="com.ibdata.project2" level="DEBUG" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    
    <Logger name="org.springframework.core" level="DEBUG" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    
    <Logger name="org.springframework.beans" level="DEBUG" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    
    <Logger name="org.springframework.context" level="DEBUG" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    
    <Logger name="org.springframework.web" level="DEBUG" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    
    <Root level="error">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>